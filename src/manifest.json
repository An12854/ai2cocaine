{
    "manifest_version": 3,
    "name": "AI2Cocaine",
    "description": "Browser extension that replaces webpage mentions of AI with Cocaine",
    "version": "1.0.0",
    "homepage_url": "https://github.com/an12854/ai2cocaine",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "index.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "An12854@users.noreply.github.com",
            "strict_min_version": "109.0"
        }
    },
    "minimum_chrome_version": "88"
}